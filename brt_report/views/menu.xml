			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Tidak Ada Data</p>
			</field>
		</record>
	

		<menuitem name="Template Report" id="report_satu" action="act_setting_template" />

	</data>
