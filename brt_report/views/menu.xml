<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

		<record model="ir.actions.act_window" id="act_setting_template">
			<field name="name"> Setting Template</field>
			<field name="res_model">brt_report.template_report</field>
			<field name="target">current</field>
			<field name="context">{}</field> <!-- {'state': 'open'} -->
			<field name="domain">[]</field>
			<field name="limit">80</field> <!-- max 80 baris dlm 1 page -->
