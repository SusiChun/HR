<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
       
        
        <record id="shift_report_xls_output_view" model="ir.ui.view">
         <field name="name">Excel Output</field>
         <field name="model">shift.report.output.wizard</field>
         <field name="arch" type="xml">
             <form string="Excel Output">
                   <separator colspan="4" string="Please save security shift by click on dowanload link !!!"/>
                   <field name="name" invisible="1"/>
                   <field name="xls_output" filename='name'/>
                   <footer>
                       <button string="Cancel" class="oe_link" special="cancel"/>
                   </footer>
             </form>
         </field>
       </record>
        
    </data>
</openerp>